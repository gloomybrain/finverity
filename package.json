{
  "name": "finverity",
  "version": "0.1.0",
  "description": "A tiny http interface to Amazon S3 with image resizing capabilities",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "start": "node dst/main.js",
    "pretest": "npm run dev:lint",
    "test": "npm run dev:test",
    "dev:start": "ts-node-dev --inspect=0.0.0.0:9229 --interval 2000 --debounce 3000 --no-deps --no-notify --watch api-spec/,src/ src/main.ts",
    "dev:lint": "eslint src/ --ext .ts",
    "dev:test": "./node_modules/.bin/mocha --require ts-node/register test/**/*.spec.ts"
  },
  "engines": {
    "node": "16.4.2"
  },
  "author": "Denis Novozhilov",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "aws-sdk": "^2.942.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "mocha": "^9.0.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
